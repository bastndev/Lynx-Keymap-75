{
  "name": "lynx-keymap-75",
  "displayName": "Lynx Keymap 75% Keyboard",
  "description": "Available now: VSCode, Cursor AI, Windsurf & Trae AI. + (Firebase Studio) & (+ AI). ",
  "version": "0.0.1",
  "publisher": "bastndev",
  "license": "MIT",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Keymaps"
  ],
  "keywords": [
    "keybindings",
    "keymap-75%",
    "Vscode",
    "AI",
    "Cursor",
    "Trae",
    "Windsurf",
    "Firebase studio",
    "Keybinding",
    "Keybindings",
    "shortcuts",
    "2025",
    "Windows",
    "Linux",
    "MacOS"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#0a0a0a",
    "theme": "dark"
  },
  "homepage": "https://github.com/bastndev/Lynx-Keymap-75#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastndev/Lynx-Keymap-75.git"
  },
  "bugs": {
    "url": "https://github.com/bastndev/Lynx-Keymap-75/issues"
  },
  "author": {
    "name": "Gohit Bastian",
    "email": "gobasdev@gmail.com",
    "url": "https://www.bastndev.com"
  },
  "activationEvents": [
    "onCommand:lynx-keymap.generateAICommit",
    "onCommand:lynx-keymap.executeAIPopup",
    "onCommand:lynx-keymap.openAIChat",
    "onCommand:lynx-keymap.createNewAISession",
    "onCommand:lynx-keymap.showAIHistory",
    "onCommand:lynx-keymap.toggleInlineSuggest"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lynx-keymap.generateAICommit",
        "title": "Generate AI Commit Message"
      },
      {
        "command": "lynx-keymap.executeAIPopup",
        "title": "Open AI Chat Interface"
      },
      {
        "command": "lynx-keymap.openAIChat",
        "title": "Open AI Chat"
      },
      {
        "command": "lynx-keymap.createNewAISession",
        "title": "Create New AI Session"
      },
      {
        "command": "lynx-keymap.showAIHistory",
        "title": "Show AI Chat History"
      },
      {
        "command": "lynx-keymap.toggleInlineSuggest",
        "title": "Toggle Inline Suggestions (AI Autocomplete)"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+1",
        "mac": "cmd+1",
        "command": "workbench.view.explorer"
      },
      {
        "key": "ctrl+2",
        "mac": "cmd+2",
        "command": "workbench.view.scm"
      },
      {
        "key": "ctrl+3",
        "mac": "cmd+3",
        "command": "workbench.view.extensions"
      },
      {
        "key": "alt+c",
        "mac": "alt+c",
        "command": "explorer.newFile"
      },
      {
        "key": "alt+v",
        "mac": "alt+v",
        "command": "explorer.newFolder"
      },
      {
        "key": "ctrl+shift+tab",
        "mac": "ctrl+shift+tab",
        "command": "workbench.action.toggleSidebarVisibility"
      },
      {
        "key": "ctrl+alt+1",
        "mac": "cmd+alt+1",
        "command": "git.stageAll",
        "when": "scmProvider == git"
      },
      {
        "key": "ctrl+alt+2",
        "mac": "ctrl+alt+2",
        "command": "lynx-keymap.generateAICommit"
      },
      {
        "key": "ctrl+alt+3",
        "mac": "cmd+alt+3",
        "command": "git.unstageAll",
        "when": "scmProvider == git"
      },
      {
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "command": "git.commit",
        "when": "scmProvider == git && sideBarFocus"
      },
      {
        "key": "ctrl+alt+enter",
        "mac": "cmd+alt+enter",
        "command": "git.push",
        "when": "scmProvider == git"
      },
      {
        "key": "alt+f",
        "mac": "alt+f",
        "command": "editor.action.formatDocument"
      },
      {
        "key": "pagedown",
        "mac": "pagedown",
        "command": "workbench.action.togglePanel"
      },
      {
        "key": "pagedown",
        "mac": "pagedown",
        "command": "workbench.action.terminal.toggleTerminal"
      },
      {
        "key": "ctrl+pagedown",
        "mac": "cmd+pagedown",
        "command": "workbench.debug.action.toggleRepl"
      },
      {
        "key": "alt+pagedown",
        "mac": "alt+pagedown",
        "command": "gitlens.showGraph"
      },
      {
        "key": "ctrl+[Backquote]",
        "mac": "cmd+[Backquote]",
        "command": "lynx-keymap.executeAIPopup"
      },
      {
        "key": "ctrl+tab",
        "mac": "cmd+tab",
        "command": "lynx-keymap.openAIChat"
      },
      {
        "key": "ctrl+alt+[Backquote]",
        "mac": "cmd+alt+[Backquote]",
        "command": "lynx-keymap.createNewAISession"
      },
      {
        "key": "ctrl+shift+[Backquote]",
        "mac": "cmd+shift+[Backquote]",
        "command": "lynx-keymap.showAIHistory"
      },
      {
        "key": "ctrl+alt+end",
        "mac": "cmd+alt+end",
        "command": "lynx-keymap.toggleInlineSuggest"
      },
      {
        "key": "ctrl+alt+backspace",
        "mac": "cmd+alt+backspace",
        "command": "inlineChat.close",
        "when": "inlineChatFocused"
      },
      {
        "key": "insert",
        "mac": "insert",
        "command": "workbench.action.debug.start"
      },
      {
        "key": "ctrl+insert",
        "mac": "ctrl+insert",
        "command": "workbench.action.debug.restart"
      },
      {
        "key": "end",
        "mac": "end",
        "command": "workbench.action.debug.stop"
      },
      {
        "key": "pageup",
        "mac": "pageup",
        "command": "git.viewChanges"
      },
      {
        "key": "ctrl+pageup",
        "mac": "cmd+pageup",
        "command": "markdown.showPreviewToSide"
      },
      {
        "key": "alt+pageup",
        "mac": "alt+pageup",
        "command": "workbench.action.closeActiveEditor"
      },
      {
        "key": "shift+escape",
        "mac": "shift+escape",
        "command": "workbench.action.closeFolder"
      },
      {
        "key": "ctrl+enter",
        "command": "-github.copilot.generate"
      }
    ]
  }
}
